{
	"name": "webext-dynamic-content-scripts-mv3",
	"version": "0.0.0",
	"manifest_version": 3,
	"permissions": ["webNavigation", "scripting"],
	"host_permissions": [
		"https://iframe-test-page.vercel.app/*",
		"https://iframe-test-page-n786423ca-fregante.vercel.app/*"
	],
	"background": {
		"service_worker": "../background.js",
		"type": "module"
	},
	"content_scripts": [
		{
			"all_frames": true,
			"matches": ["https://iframe-test-page.vercel.app/*"],
			"js": ["../content.js"],
			"css": ["../content.css"]
		},
		{
			"all_frames": true,
			"matches": ["https://fregante.github.io/pixiebrix-testing-ground/*"],
			"exclude_matches": [
				"https://fregante.github.io/pixiebrix-testing-ground/Parent-page*"
			],
			"js": ["../content.js"],
			"css": ["../content.css"]
		}
	],
	"web_accessible_resources":[
	{
		"resources":  ["*.js", "*.css"],
		"matches": [ "https://for-parcel-only.example.org/*" ]
	}]
}
