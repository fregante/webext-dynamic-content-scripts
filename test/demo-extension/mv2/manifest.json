{
	"name": "webext-dynamic-content-scripts",
	"version": "0.0.0",
	"manifest_version": 2,
	"permissions": [
		"activeTab",
		"webNavigation",
		"contextMenus",
		"https://iframe-test-page.vercel.app/*",
		"https://iframe-test-page-n786423ca-fregante.vercel.app/*"
	],
	"background": {
		"scripts": ["background.js"]
	},
	"browser_action": {},
	"content_scripts": [
		{
			"all_frames": true,
			"matches": ["https://iframe-test-page.vercel.app/*"],
			"js": ["content.js"],
			"css": ["content.css"]
		},
		{
			"all_frames": true,
			"matches": ["https://fregante.github.io/pixiebrix-testing-ground/*"],
			"exclude_matches": [
				"https://fregante.github.io/pixiebrix-testing-ground/Parent-page*"
			],
			"js": ["content.js"],
			"css": ["content.css"]
		}
	]
}
