{
	"name": "webext-dynamic-content-scripts",
	"version": "0.0.0",
	"manifest_version": 3,
	"permissions": ["tabs", "scripting", "contextMenus", "activeTab", "storage"],
	"host_permissions": [
		"https://dynamic-ephiframe.vercel.app/*",
		"https://accepted-ephiframe.vercel.app/*"
	],
	"background": {
		"service_worker": "background.js",
		"type": "module"
	},
	"action": {},
	"content_scripts": [
		{
			"all_frames": true,
			"matches": ["https://static-ephiframe.vercel.app/*"],
			"js": ["content.js"],
			"css": ["content.css"]
		},
		{
			"all_frames": true,
			"matches": ["https://partial-ephiframe.vercel.app/*"],
			"exclude_matches": [
				"https://partial-ephiframe.vercel.app/Excluded*"
			],
			"js": ["content.js"],
			"css": ["content.css"]
		}
	]
}
